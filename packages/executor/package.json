{
  "name": "executor",
  "version": "0.1.0-alpha.0",
  "description": "The Relayer module of Etherspot bundler client",
  "author": "Etherspot",
  "homepage": "https://https://github.com/etherspot/skandha#readme",
  "license": "MIT",
  "type": "module",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "*.d.ts",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/etherspot/skandha.git"
  },
  "bugs": {
